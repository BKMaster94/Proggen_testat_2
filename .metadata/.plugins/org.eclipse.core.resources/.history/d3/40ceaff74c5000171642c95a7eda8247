<html>
<head>
<script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
<script>

$("button").click(function(){
    $.post("http://localhost:8081/",
    {
        name: "Donald Duck",
        city: "Duckburg"
    },
    function(data, status){
        alert("Data: " + data + "\nStatus: " + status);
    });
});



</script>

</head>
